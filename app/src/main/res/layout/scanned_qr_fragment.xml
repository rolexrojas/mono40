<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.mono40.movil.dep.widget.Label
        android:id="@+id/label3"
        android:layout_width="80dp"
        android:layout_height="29dp"
        android:text="Modelo"
        android:textAlignment="textStart"
        app:layout_constraintBottom_toTopOf="@+id/label6"
        app:layout_constraintEnd_toStartOf="@+id/txtModelo"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label5" />

    <com.mono40.movil.dep.widget.Label
        android:id="@+id/label6"
        android:layout_width="80dp"
        android:layout_height="29dp"
        android:text="AÃ±o"
        android:textAlignment="textStart"
        app:layout_constraintBottom_toTopOf="@+id/label7"
        app:layout_constraintEnd_toStartOf="@+id/txtModelo"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label3" />

    <com.mono40.movil.dep.widget.Label
        android:id="@+id/label7"
        android:layout_width="80dp"
        android:layout_height="29dp"
        android:text="Millaje"
        android:textAlignment="textStart"
        app:layout_constraintBottom_toTopOf="@+id/scrollView2"
        app:layout_constraintEnd_toStartOf="@+id/txtAno"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label6" />

    <com.mono40.movil.dep.widget.Label
        android:id="@+id/label5"
        android:layout_width="80dp"
        android:layout_height="29dp"
        android:text="Marca"
        android:textAlignment="textStart"
        app:layout_constraintBottom_toTopOf="@+id/label3"
        app:layout_constraintEnd_toStartOf="@+id/txtMarca"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/label4" />

    <com.mono40.movil.dep.widget.Label
        android:id="@+id/label4"
        android:layout_width="80dp"
        android:layout_height="33dp"
        android:text="@string/insurance"
        android:textAlignment="viewStart"
        app:layout_constraintBottom_toTopOf="@+id/label5"
        app:layout_constraintEnd_toStartOf="@+id/txtSeguro"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.mono40.movil.dep.widget.TextInput
        android:id="@+id/txtSeguro"
        android:layout_width="196dp"
        android:layout_height="31dp"
        app:layout_constraintBottom_toTopOf="@+id/txtMarca"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/label4"
        app:layout_constraintTop_toTopOf="parent" />

    <com.mono40.movil.dep.widget.TextInput
        android:id="@+id/txtMarca"
        android:layout_width="196dp"
        android:layout_height="31dp"
        app:layout_constraintBottom_toTopOf="@+id/txtModelo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/label5"
        app:layout_constraintTop_toBottomOf="@+id/txtSeguro" />

    <com.mono40.movil.dep.widget.TextInput
        android:id="@+id/txtModelo"
        android:layout_width="196dp"
        android:layout_height="31dp"
        app:layout_constraintBottom_toTopOf="@+id/txtAno"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/label3"
        app:layout_constraintTop_toBottomOf="@+id/txtMarca" />

    <com.mono40.movil.dep.widget.TextInput
        android:id="@+id/txtAno"
        android:layout_width="196dp"
        android:layout_height="31dp"
        app:layout_constraintBottom_toTopOf="@+id/txtMillaje"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/label6"
        app:layout_constraintTop_toBottomOf="@+id/txtModelo" />

    <com.mono40.movil.dep.widget.TextInput
        android:id="@+id/txtMillaje"
        android:layout_width="196dp"
        android:layout_height="31dp"
        app:layout_constraintBottom_toTopOf="@+id/scrollView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/label7"
        app:layout_constraintTop_toBottomOf="@+id/txtAno" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="322dp"
        android:layout_height="505dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtMillaje">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RadioButton
                android:id="@+id/qradioButton"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Cambio de Aceite" />

            <RadioButton
                android:id="@+id/qradioButton2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Cambio de filtro de aceite" />

            <RadioButton
                android:id="@+id/qradioButton3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Cambio de fluido de transmission" />

            <RadioButton
                android:id="@+id/qradioButton4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Cambio de fluido de frenos" />

            <RadioButton
                android:id="@+id/qradioButton5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Cambio de fluido hidraulico" />

            <RadioButton
                android:id="@+id/qradioButton6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Cambio de fluido de enfriamiento" />

            <RadioButton
                android:id="@+id/qradioButton7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Chequeo de agua parabrisas" />

            <RadioButton
                android:id="@+id/qradioButton8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Cambio de agua baterias" />

            <RadioButton
                android:id="@+id/qradioButton9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Chequeo mangueras radiador" />

            <RadioButton
                android:id="@+id/qradioButton10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Chequeo mangueras calentador" />

            <RadioButton
                android:id="@+id/qradioButton11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Chequeo mangueras A/C" />

            <RadioButton
                android:id="@+id/qradioButton12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Cambio filtro de aire" />

            <RadioButton
                android:id="@+id/qradioButton13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Chequeo presion neumaticos" />

            <RadioButton
                android:id="@+id/qradioButton14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Chequeo desgaste neumaticos" />

            <RadioButton
                android:id="@+id/qradioButton15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Chequeo limpia vidrios" />

            <RadioButton
                android:id="@+id/qradioButton16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Chequeo de alineacion luces delanteras " />

            <RadioButton
                android:id="@+id/qradioButton17"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Revision cinturon seguridad " />

            <RadioButton
                android:id="@+id/qradioButton18"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Revision freno de mano" />
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>